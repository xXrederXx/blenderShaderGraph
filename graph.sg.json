[
	{ 
		"id": "brick",
		"type": "BrickTexture",
		"params": {
			"width": 1000,
			"height": 1000,
			"offset": 1,
			"offsetFrequency": 2,
			"squash": 2,
			"squashFrequency": 2,
			"color1": "#ffffff", 
			"color2": "#000000",
			"colorMotar": "#00ff00",
			"motarSize": 7,
			"motarSmoothness": 1,
			"bias": 0,
			"brickWidth": 100,
			"rowHeight": 100,
			"forceTilable": true
		}
	},
	{
		"id": "noiseBrickOld",
		"type": "NoiseTexture",
		"params": {
			"width": 1024,
			"height": 1024,
			"size": 1,
			"detail": 2,
			"roughness": 0
		}
	},
	{
		"id": "noiseBrick",
		"type": "TileFixer",
		"params": {
			"image": "noiseBrickOld",
			"blur": 32
		}
	},
	{
		"id": "rampBrick",
		"type": "ColorRamp",
		"params": {
			"image": "noiseBrick",
			"mode": "linear",
			"colorStops": [
				{ "color": "#000000", "position": 0 },
				{ "color": "#717171", "position": 1 }
			]
		}
	},
	{
		"id": "mixBrick",
		"type": "MixColor",
		"params": {
			"a": "brick",
			"b": "rampBrick",
			"factor": 0.442,
			"mode": "value"
		}
	},
	{
		"id": "colorizedBrick",
		"type": "ColorRamp",
		"params": {
			"image": "mixBrick",
			"colorStops": [
				{"color": "#6FFFD2", "position": 0},
				{"color": "#5686A7", "position": 0.272},
				{"color": "#6BE9AA", "position": 0.42},
				{"color": "#6FFFD2", "position": 1}
			],
			"mode": "linear"
		}
	},
	{
		"id": "noiseLine",
		"type": "NoiseTexture",
		"params": {

		}
	},
	{
		"id": "lineMix",
		"type": "ColorRamp",
		"params": {
			"image": "noiseLine",
			"colorStops": [
				{"color": "#000000", "position": 0},
				{"color": "#878787", "position": 1}
			]
		}
	},
	{
		"id": "mixLast",
		"type": "MixColor",
		"params": {
			"a": "colorizedBrick",
			"b": "lineMix",
			"factor": "brick.fac"
		}
	},
	{
		"id": "out1",
		"type": "Output",
		"params": {
			"filename": "brick.png",
			"image": "mixLast" 
		}
	},
	{
		"id": "out2",
		"type": "Output",
		"params": {
			"filename": "fac.png",
			"image": "brick" 
		}
	},
	{
		"id": "out3",
		"type": "Output",
		"params": {
			"filename": "fac.png",
			"image": "noiseBrick" 
		}
	}
]
